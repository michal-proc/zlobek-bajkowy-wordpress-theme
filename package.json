{
  "name": "projekt",
  "version": "1.0.0",
  "scripts": {
    "build-sass": "sass src/styles/index.scss assets/css/style.css",
    "watch-sass": "sass --watch src/styles/index.scss:assets/css/style.css",
    "build-js": "webpack",
    "watch-js": "webpack --watch",
    "watch": "concurrently \"npm run watch-sass\" \"npm run watch-js\"",
    "build": "concurrently \"npm run build-sass\" \"npm run build-js\""
  },
  "devDependencies": {
    "sass": "^1.32.0",
    "concurrently": "^7.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "jquery": "^3.7.1",
    "slick-carousel": "^1.8.1"
  }
}
